<?xml version="1.0"?>

<PropertyList>
	<path>Radar.ac</path>
	
<!-- RADAR -->
<animation>
	<type>rotate</type>
	<object-name>Radar_Base</object-name>
	<property>/sim/time/elapsed-sec</property>
		<factor>50.0</factor>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
</animation>

</PropertyList>
